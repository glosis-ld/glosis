#################################################################
#    Object Properties
#################################################################

###  http://w3id.org/glosis/model/siteplot#altitude
:altitude rdf:type owl:ObjectProperty ;
          rdfs:range <http://qudt.org/schema/qudt/QuantityValue> .


###  http://w3id.org/glosis/model/siteplot#country
:country rdf:type owl:ObjectProperty ;
         rdfs:range <https://www.omg.org/spec/LCC/Countries/CountryRepresentation/Alpha2Code> .


###  http://w3id.org/glosis/model/siteplot#positionalAccuracy
:positionalAccuracy rdf:type owl:ObjectProperty ;
                    rdfs:range <http://www.w3.org/ns/ssn/Property> .


###  http://w3id.org/glosis/model/siteplot#responsibleOrganization
:responsibleOrganization rdf:type owl:ObjectProperty ;
                         rdfs:range <http://xmlns.com/foaf/0.1/Agent> .


###  http://w3id.org/glosis/model/siteplot#timestamp
:timestamp rdf:type owl:ObjectProperty .


#################################################################
#    Data properties
#################################################################

###  http://w3id.org/glosis/model/siteplot#cropDescription
:cropDescription rdf:type owl:DatatypeProperty ;
                 rdfs:range xsd:string ;
                 <http://www.w3.org/2004/02/skos/core#definition> "Description of crops at the site"@en .


###  http://w3id.org/glosis/model/siteplot#landUseCropRotation
:landUseCropRotation rdf:type owl:DatatypeProperty ;
                     rdfs:range xsd:string ;
                     <http://www.w3.org/2004/02/skos/core#definition> "Description of crop rotation at the site"@en .


###  http://w3id.org/glosis/model/siteplot#landUseRemark
:landUseRemark rdf:type owl:DatatypeProperty ;
               rdfs:range xsd:string ;
               <http://www.w3.org/2004/02/skos/core#definition> "Description of land use at the site"@en .


###  http://w3id.org/glosis/model/siteplot#location
:location rdf:type owl:DatatypeProperty ;
          rdfs:range xsd:string .


###  http://w3id.org/glosis/model/siteplot#mapSheetID
:mapSheetID rdf:type owl:DatatypeProperty ;
            rdfs:range xsd:string .


###  http://w3id.org/glosis/model/siteplot#parentRemarks
:parentRemarks rdf:type owl:DatatypeProperty ;
               rdfs:range xsd:string ;
               <http://www.w3.org/2004/02/skos/core#definition> "Remarks on the parent material"@en .


###  http://w3id.org/glosis/model/siteplot#physiographyDescription
:physiographyDescription rdf:type owl:DatatypeProperty ;
                         rdfs:range xsd:string ;
                         <http://www.w3.org/2004/02/skos/core#definition> "Description of the local physiography"@en .


###  http://w3id.org/glosis/model/siteplot#remarks
:remarks rdf:type owl:DatatypeProperty ;
         rdfs:range xsd:string .


###  http://w3id.org/glosis/model/siteplot#timestamp
:timestamp rdf:type owl:DatatypeProperty ;
           rdfs:range xsd:dateTime .


###  http://w3id.org/glosis/model/siteplot#vegetationDescription
:vegetationDescription rdf:type owl:DatatypeProperty ;
                       rdfs:range xsd:string ;
                       <http://www.w3.org/2004/02/skos/core#definition> "Description of vegetation at the site"@en .


#################################################################
#    Classes
#################################################################

###  http://qudt.org/schema/qudt/Unit
<http://qudt.org/schema/qudt/Unit> rdf:type owl:Class .


###  http://w3id.org/glosis/model/common#SoilDepth
<http://w3id.org/glosis/model/common#SoilDepth> rdfs:subClassOf [ rdf:type owl:Restriction ;
                                                                  owl:onProperty <http://www.w3.org/ns/sosa/hasFeatureOfInterest> ;
                                                                  owl:allValuesFrom [ rdf:type owl:Class ;
                                                                                      owl:unionOf ( :GL_Plot
                                                                                                    :GL_Site
                                                                                                  )
                                                                                    ]
                                                                ] .


