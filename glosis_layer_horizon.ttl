                [owl:unionOf (glosis_lh:GL_Layer glosis_lh:GL_Horizon) ] ] ;
        rdfs:subClassOf [ a owl:Restriction ; owl:onProperty
            sosa:hasResult ; owl:allValuesFrom glosis_lh:MoistureContentValue ] ;
        rdfs:subClassOf [ a owl:Restriction ; owl:onProperty
            sosa:observedProperty ; owl:hasValue glosis_lh:moistureContentProperty ] ;
