            sosa:observedProperty ; owl:hasValue glosis_cl:physioChemicalPropertyCode-pH ] ;
        rdfs:subClassOf [ a owl:Restriction ; owl:onProperty
            sosa:usedProcedure ; owl:someValuesFrom glosis_proc:PHProcedure ] .

glosis_lh:PHValue a owl:Class ;
