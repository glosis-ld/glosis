                                                                                 ] ,
                                                                                 [ rdf:type owl:Restriction ;
                                                                                   owl:onProperty <http://www.w3.org/ns/sosa/hasFeatureOfInterest> ;
                                                                                   owl:allValuesFrom [ rdf:type owl:Class ;
                                                                                                       owl:unionOf ( <http://w3id.org/glosis/model/siteplot/GL_Plot>
                                                                                                                     <http://w3id.org/glosis/model/siteplot/GL_Site>
                                                                                                                   )
                                                                                                     ]
                                                                                 ] ,
                                                                                 [ rdf:type owl:Restriction ;
                                                                                   owl:onProperty <http://www.w3.org/ns/sosa/observedProperty> ;
                                                                                   owl:hasValue <http://w3id.org/glosis/model/siteplot/erosionTotalAreaAffectedProperty>
