<http://w3id.org/glosis/model/siteplot>
        a                owl:Ontology ;
        owl:imports      ssn: , sosa: , glosis_cl: , glosis_cm: , iso28258: ;
        owl:versionIRI   <http://w3id.org/glosis/model/siteplot/4.0> ;
        owl:versionInfo  "4.0" ;
